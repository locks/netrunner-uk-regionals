<div class="col-md-8 offset-md-2 p-3 p-md-5">
<h1><strong><a href="{{page.url}}">{{title}}</a></strong></h1>
<h2>{{description}}</h2>
<hr/>
<div class="p-3 rounded p-md-5 bg-light">
{% for pricing in price %}
<h3><a href="{{page.url}}">{{pricing.name}}</a></h3>
<p>{{pricing.fitur}}</p>
<p><a href="{{pricing.link}}" class="border p-1 rounded">Order {{pricing.name}}</a></p>
<hr/>
{% endfor %}
</div>
</div>